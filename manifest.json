{
	"manifest_version": 2,

	"name": "Memory Manager",
	"description": "This extension gets allocated memory of processes from Chrome's task manager.",
	"icons": {"38": "rsz_38speedicon"},

	"version": "1.0",

	"browser_action": {
		"default_icon": "Images/rsz_speedicon.png",
		"default_popup": "popup.html",
		"default_title": "Memory Manager"
	},

	"permissions": [
	"tabs",
	"processes",
	"system.memory",
	"bookmarks",
	"https://ajax.googleapis.com/"
	]
}